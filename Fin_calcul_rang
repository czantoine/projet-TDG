for i in range(1,len(Tar)):
    compteur=0
    for j in range(1,len(Tar[0])):
        for k in Lpe:
            if k==Ta[0][j]:
                Vald =  True
                break
            else:
                Vald = False
        if Vald == True :
            Ld = Tar[i]
            del Ld[j+compteur]
            compteur = compteur - 1
            Tar[i]=Ld
Lv = []
for i in range (len(Li)):
    for k in Lpe:
        if Li[i] ! = k:
            ValL = True
            break
        else : 
            ValL = False
            break
    if ValL == True
        Lv.append(Li[i])
Tar[0] = Lv
Lf = Lf+[Lpe]
return calcul_rang(Tar,Lf)
            